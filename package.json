{
  "name": "morning-winner",
  "version": "1.0.0",
  "description": "Discord朝活トラッキングBot - ボイスチャンネル参加状況をGoogle Sheetsで記録・可視化",
  "main": "bot.js",
  "type": "module",
  "scripts": {
    "start": "node bot.js",
    "dev": "node --watch bot.js",
    "setup": "node setup-sheets.js",
    "register-commands": "node register-commands.js",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop morning-winner-bot",
    "pm2:restart": "pm2 restart morning-winner-bot",
    "pm2:logs": "pm2 logs morning-winner-bot",
    "pm2:status": "pm2 status",
    "pm2:startup": "pm2 startup",
    "pm2:save": "pm2 save"
  },
  "keywords": [
    "discord",
    "bot",
    "tracking",
    "google-sheets",
    "morning-routine"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "googleapis": "^130.0.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "pm2": "^5.4.2"
  }
}
